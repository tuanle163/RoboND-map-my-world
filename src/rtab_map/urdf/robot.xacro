<?xml version='1.0'?>

<!-- Robot -->

<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- import URDF files -->
  <xacro:include filename="$(find rtab_map)/urdf/common_properties.xacro"/>
  <xacro:include filename="$(find rtab_map)/urdf/robot.gazebo" />

  <!-- robot_footprint -->
  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <!-- chassis -->
  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0.025" rpy=" 0 0 0"/>
      <geometry>
        <box size=".4 .2 .15"/>
      </geometry>
      <material name="green"/>
    </visual>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0.025" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".4 .2 .15"/>
      </geometry>
    </collision>

    <!-- back_caster_collision -->

    <visual name='back_caster_visual'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </visual>

    <collision name='back_caster_collision'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <!-- front_caster_collision -->

    <visual name='front_caster_visual'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
    </visual>

    <collision name='front_caster_collision'>
      <origin xyz="0.15 0 -0.05" rpy=" 0 0 0"/>
      <geometry>
        <sphere radius="0.0499"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

  </link>

  <!-- left_wheel -->
 
  <link name="left_wheel">

    <visual name='left_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

   <collision name='left_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
   </collision>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

  </link>

  <joint type="continuous" name="left_wheel_hinge">
      <origin xyz="0 0.15 0" rpy="0 0 0"/>
      <child link="left_wheel"/>
      <parent link="chassis"/>
      <axis xyz="0 1 0" rpy="0 0 0"/>
      <limit effort="10000" velocity="1000"/>
      <joint_properties damping="1.0" friction="1.0"/>
  </joint>


  <!-- right_wheel -->
    
  <link name="right_wheel">

    <visual name='right_wheel_visual'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>

    <collision name='right_wheel_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length="0.05"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

  </link>

  <joint type="continuous" name="right_wheel_hinge">
      <origin xyz="0 -0.15 0" rpy="0 0 0"/>
      <child link="right_wheel"/>
      <parent link="chassis"/>
      <axis xyz="0 1 0" rpy="0 0 0"/>
      <limit effort="10000" velocity="1000"/>
      <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <!-- RGBD camera -->

  <!-- RGB camera link -->
  <link name="RGBD_camera">
    
    <visual name='RGBD_camera_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename="package://rtab_map/meshes/kinect.dae"/>
      </geometry>
    </visual>
    
    <collision>
      <origin xyz="-0.03 0 -0.015" rpy="0 0 0"/>
  	  <geometry>
  	    <box size="0.075 0.25 0.075"/>
  	  </geometry>
  	</collision>
    
    <inertial>
      <mass value="0.564" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.003881243" ixy="0.0" ixz="0.0"
               iyy="0.000498940" iyz="0.0"
               izz="0.003879257" />
    </inertial>
  </link>

  <!-- Depth camera link - used to rotate point cloud -->
  <link name="Depth_camera"></link>

  <!-- RGB camera joint -->
  <joint type="fixed" name="RGB_camera_joint">
      <origin xyz="0.2 0 0.15" rpy="0 0 0"/>
      <child link="RGBD_camera"/>
      <parent link="chassis"/>
      <axis xyz="1 0 0" rpy="0 0 0"/>
  </joint>

  <!-- Depth camera joint -->
  <joint type="fixed" name="Depth_camera_joint">
      <origin xyz="0.2 0 0.15" rpy="${-PI/2} 0 ${-PI/2}"/>
      <child link="Depth_camera"/>
      <parent link="RGBD_camera"/>
      <axis xyz="1 0 0" rpy="0 0 0"/>
  </joint>

  <!-- hokuyo laser range finder -->
  <link name="hokuyo">
    <visual name='hokuyo_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename="package://rtab_map/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

    <collision name='hokuyo_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.075"/>
      </geometry>
    </collision>

    <visual name='hokuyo_base_visual'>
      <origin xyz="0 0 -0.0925" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.115"/>
      </geometry>
    </visual>

    <collision name='hokuyo_base_collision'>
      <origin xyz="0 0 -0.0925" rpy=" 0 0 0"/>
      <geometry>
        <box size="0.05 0.05 0.115"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>
  </link>

  <joint type="fixed" name="hokuyo_joint">
      <origin xyz="0.1 0 0.25" rpy="0 0 0"/>
      <child link="hokuyo"/>
      <parent link="chassis"/>
      <axis xyz="1 0 0" rpy="0 0 0"/>
  </joint>

</robot>
